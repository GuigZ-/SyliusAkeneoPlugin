{
    "name": "synolia/sylius-akeneo-plugin",
    "type": "sylius-plugin",
    "keywords": [
        "sylius",
        "sylius-plugin",
        "akeneo",
        "connector"
    ],
    "description": "Akeneo connector for Sylius.",
    "license": "EUPL-1.2",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "akeneo/api-php-client": "^4.0||^5.0||^6.0",
        "akeneo/api-php-client-ee": "^4.0||^5.0||^6.0",
        "behat/transliterator": "^1.3",
        "bluepsyduck/symfony-process-manager": "^1.3",
        "doctrine/dbal": "^2.10",
        "doctrine/doctrine-bundle": "^1.12 || ^2.0",
        "http-interop/http-factory-guzzle": "^1.0",
        "league/pipeline": "^1.0",
        "php-http/guzzle6-adapter": "^2.0",
        "psr/event-dispatcher": "^1.0",
        "sylius/sylius": "^1.9",
        "symfony/framework-bundle": "^4.4|^5.1",
        "symfony/lock": "^4.3|^5.1",
        "symfony/property-access": "^4.3|^5.1",
        "symfony/property-info": "^4.3|^5.1",
        "symfony/serializer": "^4.3|^5.1",
        "symfony/service-contracts": "^1.1|^2.0",
        "webmozart/assert": "^1.8"
    },
    "require-dev": {
        "behat/behat": "3.11.0",
        "behat/mink": "1.10.0",
        "behat/mink-browserkit-driver": "1.4.1",
        "behat/mink-extension": "2.3.1",
        "behat/mink-selenium2-driver": "1.6.0",
        "donatj/mock-webserver": "2.5.0",
        "friends-of-behat/page-object-extension": "0.3.2",
        "friends-of-behat/suite-settings-extension": "1.1.0",
        "friends-of-behat/symfony-extension": "2.3.1",
        "friends-of-behat/variadic-extension": "1.5.0",
        "friendsoftwig/twigcs": "5.1.0",
        "j13k/yaml-lint": "1.1.4",
        "php-parallel-lint/php-parallel-lint": "1.3.2",
        "lakion/mink-debug-extension": "2.0.0",
        "phpmd/phpmd": "2.12.0",
        "phpro/grumphp": "1.5.1",
        "phpspec/phpspec": "7.2.0",
        "phpstan/extension-installer": "1.1.0",
        "phpstan/phpstan": "0.12.91",
        "phpstan/phpstan-doctrine": "0.12.41",
        "phpstan/phpstan-webmozart-assert": "0.12.14",
        "phpunit/phpunit": "9.5.21",
        "sebastian/phpcpd": "6.0.3",
        "seld/jsonlint": "1.9.0",
        "slevomat/coding-standard": "6.4.1",
        "sylius-labs/coding-standard": "4.0.3",
        "symfony/browser-kit": "4.4.37",
        "symfony/debug-bundle": "4.4.37",
        "symfony/dotenv": "4.4.37",
        "symfony/intl": "4.4.43",
        "symfony/web-profiler-bundle": "4.4.43",
        "symfony/web-server-bundle": "4.4.37"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Synolia\\SyliusAkeneoPlugin\\": "src/",
            "Tests\\Synolia\\SyliusAkeneoPlugin\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "symfony/thanks": true,
            "phpro/grumphp": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "symfony": {
            "require": "^4.3 || ^5.2"
        }
    },
    "scripts": {
        "fix-ecs": "ecs check -c ruleset/ecs.php --fix --ansi --clear-cache",
        "patch": "php bin/patch"
    },
    "conflict": {
        "doctrine/dbal": "^3.0"
    }
}
